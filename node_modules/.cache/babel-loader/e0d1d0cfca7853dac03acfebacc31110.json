  <v-banner color=\"blue\" lines=\"two\" max-width=\"800\"\n            ><v-icon slot=\"icon\" color=\"black\" size=\"48\"\n              >mdi-package-variant</v-icon\n            >\n            <v-banner-text class=\"text-left\">\n              <h2>Pendataan Bantuan Sosial Kemensos</h2>\n            </v-banner-text>\n          </v-banner>\n        </div>\n      </div>\n\n      <v-card\n        class=\"mx-auto px-7 py-10 rounded-b-lg rounded-t-0\"\n        max-width=\"800\"\n        outlined\n        elevation=\"3\"\n      >\n        <v-form\n          ref=\"form\"\n          v-model=\"valid\"\n          @submit.prevent=\"submit\"\n          lazy-validation\n        >\n          <v-text-field\n            v-model=\"nama\"\n            class=\"mb-4\"\n            :rules=\"[(v) => !!v || 'Nama wajib Diisi']\"\n            label=\"Nama Lengkap\"\n            color=\"accent\"\n            required\n          ></v-text-field>\n\n          <v-text-field\n            v-model=\"nik\"\n            name=\"nik\"\n            class=\"mb-4\"\n            :counter=\"16\"\n            @keypress=\"cekNumber\"\n            :rules=\"[(v) => !!v || 'NIK wajib diisi', rules.nik]\"\n            label=\"NIK\"\n            color=\"accent\"\n            type=\"number\"\n            required\n          ></v-text-field>\n\n          <v-text-field\n            v-model=\"noKK\"\n            name=\"kk\"\n            class=\"mb-4\"\n            :counter=\"16\"\n            @keypress=\"cekNumber\"\n            :rules=\"[\n              (v) => !!v || 'Nomor Kartu Keluarga wajib diisi',\n              rules.kk,\n            ]\"\n            label=\"Nomor Kartu Keluarga\"\n            color=\"accent\"\n            type=\"number\"\n            required\n          ></v-text-field>\n\n          <div class=\"row mb-4\">\n            <div class=\"col-5 col-md-3\">\n              <v-img\n                v-if=\"fotoKTPPreview\"\n                class=\"rounded-lg elevation-2\"\n                max-width=\"150\"\n                max-height=\"150\"\n                :src=\"fotoKTPPreview\"\n                lazy-src=\"https://via.placeholder.com/150x150?text=KTP\"\n              >\n                <template v-slot:placeholder>\n                  <v-row\n                    class=\"fill-height ma-0\"\n                    align=\"center\"\n                    justify=\"center\"\n                  >\n                    <v-progress-circular\n                      indeterminate\n                      color=\"grey lighten-5\"\n                    ></v-progress-circular>\n                  </v-row>\n                </template>\n              </v-img>\n            </div>\n\n            <div class=\"col-7 col-md-9\">\n              <v-file-input\n                show-size\n                ref=\"ktp\"\n                @change=\"cekKTP\"\n                v-model=\"fotoKTP\"\n                prepend-icon=\"\"\n                :rules=\"[\n                  (v) => !!v || 'Foto KTP wajib diisi',\n                  rules.photoSize,\n                  rules.photoType,\n                ]\"\n                accept=\"image/png, image/jpeg, image/jpeg, image/bmp\"\n                label=\"Foto KTP\"\n                color=\"accent\"\n              >\n              </v-file-input>\n              <div>\n                <div\n                  class=\"text-caption text--secondary font-weight-light font-italic d-flex\"\n                  v-for=\"(message, index) in messagesFoto\"\n                  :key=\"`message-${index}`\"\n                >\n                  <div>\n                    <span v-for=\"(n, i) in index + 1\" :key=\"i + 1\">*</span>\n                  </div>\n                  <div>\n                    <span> &nbsp;{{ message }} </span>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"row mb-4\">\n            <div class=\"col-5 col-md-3\">\n              <v-img\n                class=\"rounded-lg elevation-2\"\n                max-width=\"150\"\n                max-height=\"150\"\n                load\n                :src=\"fotoKKPreview\"\n                lazy-src=\"https://via.placeholder.com/150x150?text=KK\"\n              >\n                <template v-slot:placeholder>\n                  <v-row\n                    class=\"fill-height ma-0\"\n                    align=\"center\"\n                    justify=\"center\"\n                  >\n                    <v-progress-circular\n                      indeterminate\n                      color=\"grey lighten-5\"\n                    ></v-progress-circular>\n                  </v-row>\n                </template>\n              </v-img>\n            </div>\n\n            <div class=\"col-7 col-md-9\">\n              <v-file-input\n                show-size\n                ref=\"kk\"\n                @change=\"cekKK\"\n                v-model=\"fotoKK\"\n                prepend-icon=\"\"\n                :rules=\"[\n                  (v) => !!v || 'Foto KK wajib diisi',\n                  rules.photoSize,\n                  rules.photoType,\n                ]\"\n                accept=\"image/png, image/jpeg, image/jpeg, image/bmp\"\n                label=\"Foto Kartu Keluarga\"\n                color=\"accent\"\n              >\n              </v-file-input>\n              <div>\n                <div\n                  class=\"text-caption text--secondary font-weight-light font-italic d-flex\"\n                  v-for=\"(message, index) in messagesFoto\"\n                  :key=\"`message-${index}`\"\n                >\n                  <div>\n                    <span v-for=\"(n, i) in index + 1\" :key=\"i + 1\">*</span>\n                  </div>\n                  <div>\n                    <span> &nbsp;{{ message }} </span>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n\n          <div class=\"row mb-4\">\n            <v-text-field\n              v-model=\"umur\"\n              class=\"col-12 col-sm-6\"\n              :rules=\"[(v) => !!v || 'Umur wajib diisi', rules.umur]\"\n              label=\"Umur (Min. 25 Tahun)\"\n              type=\"number\"\n              color=\"accent\"\n              required\n            ></v-text-field>\n\n            <div class=\"col-12 col-sm-6\">\n              <div class=\"label-radio\">\n                <span>Jenis Kelamin</span>\n              </div>\n              <v-radio-group\n                v-model=\"jenisKelamin\"\n                mandatory\n                row\n                hide-details\n                class=\"ma-0 ma-mb-0 mt-n3\"\n              >\n                <v-radio\n                  label=\"Laki-laki\"\n                  value=\"Laki-laki\"\n                  color=\"accent\"\n                ></v-radio>\n                <v-radio\n                  label=\"Perempuan\"\n                  value=\"Perempuan\"\n                  color=\"accent\"\n                ></v-radio>\n              </v-radio-group>\n            </div>\n          </div>\n\n          <div class=\"row mb-4\">\n            <v-select\n              class=\"col-6\"\n              v-model=\"provinsi\"\n              @change=\"onchangeProvinsi\"\n              :items=\"provinsidata\"\n              item-text=\"name\"\n              item-value=\"id\"\n              :rules=\"[(v) => !!v || 'Provinsi wajib diisi']\"\n              label=\"Pilih Provinsi\"\n              color=\"accent\"\n              attach\n              auto\n              required\n            ></v-select>\n            <v-select\n              class=\"col-6\"\n              :disabled=\"kotadata.length == 0\"\n              v-model=\"kota\"\n              @change=\"onchangeKota\"\n              :items=\"kotadata\"\n              item-text=\"name\"\n              item-value=\"id\"\n              :rules=\"[(v) => !!v || 'Kota/Kab. wajib diisi']\"\n              label=\"Pilih Kota/Kab.\"\n              color=\"accent\"\n              attach\n              auto\n              required\n            ></v-select>\n          </div>\n\n          <div class=\"row mb-4\">\n            <v-select\n              class=\"col-6\"\n              :disabled=\"kecamatandata.length == 0\"\n              v-model=\"kecamatan\"\n              @change=\"onchangeKecamatan\"\n              :items=\"kecamatandata\"\n              item-text=\"name\"\n              item-value=\"id\"\n              :rules=\"[(v) => !!v || 'Kecamatan wajib diisi']\"\n              label=\"Pilih Kecamatan\"\n              color=\"accent\"\n              attach\n              auto\n              required\n            ></v-select>\n            <v-select\n              class=\"col-6\"\n              :disabled=\"desadata.length == 0\"\n              v-model=\"desa\"\n              :items=\"desadata\"\n              item-text=\"name\"\n              item-value=\"id\"\n              :rules=\"[(v) => !!v || 'Kelurahan/Desa wajib diisi']\"\n              label=\"Pilih Kelurahan/Desa\"\n              color=\"accent\"\n              attach\n              auto\n              required\n            ></v-select>\n          </div>\n\n          <v-textarea\n            v-model=\"alamat\"\n            class=\"mb-4\"\n            :rules=\"[(v) => !!v || 'Alamat wajib diisi', rules.alamat]\"\n            :counter=\"255\"\n            label=\"Alamat\"\n            color=\"accent\"\n            rows=\"1\"\n            no-resize\n            auto-grow\n          ></v-textarea>\n\n          <div class=\"row mb-4 \">\n            <v-text-field\n              v-model=\"RT\"\n              @keypress=\"cekNumber\"\n              class=\"col-6\"\n              :rules=\"[(v) => !!v || 'RT wajib Diisi']\"\n              label=\"RT\"\n              type=\"number\"\n              color=\"accent\"\n              required\n            ></v-text-field>\n            <v-text-field\n              v-model=\"RW\"\n              @keypress=\"cekNumber\"\n              class=\"col-6\"\n              :rules=\"[(v) => !!v || 'RW wajib Diisi']\"\n              label=\"RW\"\n              type=\"number\"\n              color=\"accent\"\n              required\n            ></v-text-field>\n          </div>\n\n          <div class=\"row mb-4 \">\n            <v-text-field\n              v-model=\"PengSebelumPandemi\"\n              @keypress=\"cekNumber\"\n              class=\"col-12 col-sm-6 mb-4 mb-sm-0\"\n              :rules=\"[(v) => !!v || 'Penghasilan Sebelum Pandemi wajib Diisi']\"\n              counter\n              label=\"Penghasilan Sebelum Pandemi (Rupiah)\"\n              color=\"accent\"\n              type=\"number\"\n              required\n            ></v-text-field>\n\n            <v-text-field\n              v-model=\"PengSetelahPandemi\"\n              @keypress=\"cekNumber\"\n              class=\"col-12 col-sm-6\"\n              :rules=\"[(v) => !!v || 'Penghasilan Setelah Pandemi wajib Diisi']\"\n              counter\n              label=\"Penghasilan Setelah Pandemi (Rupiah)\"\n              color=\"accent\"\n              type=\"number\"\n              required\n            ></v-text-field>\n          </div>\n\n          <div class=\"mb-4\">\n            <v-select\n              v-model=\"alasanTerpilih\"\n              :items=\"alasanList\"\n              :rules=\"[(v) => !!v || 'Alasan wajib diisi']\"\n              label=\"Alasan membutuhkan bantuan\"\n              color=\"accent\"\n              :menu-props=\"{ overflowX: true }\"\n              attach\n              auto\n              required\n            ></v-select>\n\n            <v-scale-transition>\n              <v-text-field\n                v-model=\"alasanLainnya\"\n                v-if=\"alasanTerpilih == 'Lainnya'\"\n                class=\"mt-n3\"\n                :rules=\"[(v) => !!v || 'Alasan wajib diisi']\"\n                color=\"accent\"\n                required\n              ></v-text-field>\n            </v-scale-transition>\n          </div>\n\n          <v-checkbox\n            v-model=\"check\"\n            class=\"mb-6\"\n            id=\"agreement\"\n            :rules=\"[(v) => !!v || 'Harap setujui untuk melanjutkan!']\"\n            color=\"accent\"\n            :ripple=\"false\"\n            required\n          >\n            <template v-slot:label>\n              Saya menyatakan bahwa data yang diisikan adalah benar dan siap\n              mempertanggungjawabkan apabila ditemukan ketidaksesuaian dalam\n              data tersebut.\n            </template>\n          </v-checkbox>\n\n          <div v-show=\"!valid\" class=\"error--text my-2 text-subtitle\">\n            <span>\n              Mohon Pastikan Data telah terisi seluruhnya!\n            </span>\n          </div>\n          <div class=\"text-center text-md-left\">\n            <v-btn\n              :disabled=\"!valid\"\n              :block=\"$vuetify.breakpoint.mobile\"\n              :loading=\"loadingButton\"\n              elevation=\"3\"\n              color=\"success\"\n              class=\"mr-4 rounded-pill px-16 py-6 px-md-14\"\n              type=\"submit\"\n            >\n              Simpan\n            </v-btn>\n          </div>\n        </v-form>\n      </v-card>\n    </v-container>\n  </v-form>\n</template>\n\n<script>\nimport { mapActions } from \"vuex\";\n\nexport default {\n  data: () => ({\n    valid: true,\n    loadingButton: false,\n    nama: \"\",\n    nik: \"\",\n    noKK: \"\",\n    fotoKTP: null,\n    fotoKTPPreview: \"https://via.placeholder.com/150x150?text=KTP\",\n    fotoKKPreview: \"https://via.placeholder.com/150x150?text=KK\",\n    fotoKK: null,\n    umur: \"\",\n    alamat: \"\",\n    jenisKelamin: \"\",\n    provinsi: \"\",\n    provinsidata: [],\n    kota: \"\",\n    kotadata: [],\n    kecamatan: \"\",\n    kecamatandata: [],\n    desa: \"\",\n    desadata: [],\n    RT: \"\",\n    RW: \"\",\n    PengSebelumPandemi: \"\",\n    PengSetelahPandemi: \"\",\n    alasanTerpilih: null,\n    alasanLainnya: \"\",\n    alasanList: [\n      \"Kehilangan Pekerjaan\",\n      \"Kepala keluarga terdampak atau korban Covid-19\",\n      \"Tergolong fakir/miskin semenjak sebelum Covid-19\",\n      \"Lainnya\",\n    ],\n\n    check: false,\n    messagesFoto: [\n      \"Foto tidak boleh lebih dari 2MB\",\n      \"Format file PNG/JPG/JPEG/BMP\",\n    ],\n    rules: {\n      photoSize: (v) => !v || v.size <= 2000000 || \"Ukuran file terlalu besar\",\n      photoType: (v) =>\n        !v ||\n        [\"image/png\", \"image/jpeg\", \"image/jpeg\", \"image/bmp\"].indexOf(\n          v.type\n        ) >= 0 ||\n        \"Format file salah\",\n      umur: (v) => (v && v >= 25) || \"Umur harus 25 atau keatas\",\n      nik: (v) => (v && v.length == 16) || \"NIK harus berjumlah 16 digit\",\n      kk: (v) => (v && v.length == 16) || \"Nomor KK harus berjumlah 16 digit\",\n      alamat: (v) =>\n        (v && v.length <= 255) || \"Alamat tidak boleh lebih dari 255 karakter\",\n    },\n    listCountry: [],\n    listState: [],\n    listCities: [],\n    selectedCountry: \"\",\n    selectedState: \"\",\n    selectedCity: \"\",\n    authToken: \"\",\n    names: [],\n    country_select: \"\",\n    id: \"\",\n    info: null,\n  }),\n\n  computed: {\n    statusKTP() {\n      return this.$refs.ktp;\n    },\n    statusKK() {\n      return this.$refs.kk;\n    },\n    inputedData() {\n      return {\n        nama: this.nama,\n        nik: this.nik,\n        noKK: this.noKK,\n        fotoKTP: this.fotoKTP,\n        fotoKK: this.fotoKK,\n        umur: this.umur,\n        alamat: this.alamat,\n        jenisKelamin: this.jenisKelamin,\n        RT: this.RT,\n        RW: this.RW,\n        PengSebelumPandemi: this.PengSebelumPandemi,\n        PengSetelahPandemi: this.PengSetelahPandemi,\n        alasan: this.alasan,\n      };\n    },\n    alasan() {\n      if (this.alasanTerpilih && this.alasanTerpilih == \"Lainnya\") {\n        return this.alasanLainnya;\n      }\n\n      return this.alasanTerpilih;\n    },\n  },\n\n  methods: {\n    async getData() {\n      await fetch(\n        `https://ekiditapermana.github.io/api-wilayah-indonesia/api/provinces.json`\n      )\n        .then((response) => response.json())\n        .then((provinces) => {\n          console.log(provinces);\n          this.provinsidata = provinces;\n        });\n    },\n\n    async onchangeProvinsi() {\n      await fetch(\n        `https://ekiditapermana.github.io/api-wilayah-indonesia/api/regencies/${this.provinsi}.json`\n      )\n        .then((response) => response.json())\n        .then((regencies) => {\n          console.log(regencies);\n          this.kotadata = regencies;\n          this.kota = 0;\n          this.kecamatan = 0;\n          this.desa = 0;\n        });\n    },\n\n    async onchangeKota() {\n      await fetch(\n        `https://ekiditapermana.github.io/api-wilayah-indonesia/api/districts/${this.kota}.json`\n      )\n        .then((response) => response.json())\n        .then((districts) => {\n          console.log(districts);\n          this.kecamatandata = districts;\n          this.kecamatan = 0;\n          this.desa = 0;\n        });\n    },\n\n    async onchangeKecamatan() {\n      await fetch(\n        `https://ekiditapermana.github.io/api-wilayah-indonesia/api/villages/${this.kecamatan}.json`\n      )\n        .then((response) => response.json())\n        .then((villages) => {\n          console.log(villages);\n          this.desadata = villages;\n          this.desa = 0;\n        });\n    },\n\n    ...mapActions({\n      setDialogStatus: \"dialog/setStatus\",\n    }),\n\n    sleep(ms) {\n      return new Promise((resolve) => setTimeout(resolve, ms));\n    },\n\n    randomNumber(min, max) {\n      return Math.random() * (max - min) + min;\n    },\n\n    async submit() {\n      if (this.$refs.form.validate()) {\n        let time = Math.floor(this.randomNumber(1400, 1600));\n        console.log({\n          responseTime: time,\n          data: this.inputedData,\n        });\n        this.loadingButton = true;\n        await this.sleep(time);\n        if (time >= 1500) {\n          this.loadingButton = false;\n          this.setDialogStatus(true);\n        } else {\n          this.loadingButton = false;\n          this.$router.push({\n            name: \"Info\",\n            params: {\n              savedForm: true,\n            },\n          });\n        }\n      }\n    },\n\n    cekKTP() {\n      if (this.statusKTP.validate()) {\n        const reader = new FileReader();\n        reader.onload = (e) => {\n          this.fotoKTPPreview = e.target.result;\n        };\n        reader.readAsDataURL(this.fotoKTP);\n      } else {\n        this.fotoKTPPreview = \"https://via.placeholder.com/150x150?text=KK\";\n      }\n    },\n\n    cekKK() {\n      if (this.statusKK.validate()) {\n        const reader = new FileReader();\n\n        reader.onload = (e) => {\n          this.fotoKKPreview = e.target.result;\n        };\n        reader.readAsDataURL(this.fotoKK);\n      } else {\n        this.fotoKKPreview = \"https://via.placeholder.com/150x150?text=KK\";\n      }\n    },\n\n    cekNumber(event) {\n      if (\n        (event.which != 8 && event.which != 0 && event.which < 48) ||\n        event.which > 57\n      ) {\n        event.preventDefault();\n      }\n\n      if (event.target.name == \"nik\" || event.target.name == \"kk\") {\n        if (event.target.value.length >= 16) {\n          event.preventDefault();\n        }\n      }\n    },\n  },\n\n  created() {\n    this.getData();\n  },\n\n  beforeMount() {\n    document.title = this.$route.meta.title;\n  },\n};\n</script>\n"],"sourceRoot":"src/views"}]}